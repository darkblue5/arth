							<van-sidebar-item title="{{item}}" custom-class="sb-width" info="{{ count0}}" />
						</block>
						<block wx:elif="{{index == 1}}">
							<van-sidebar-item title="{{item}}" custom-class="sb-width" info="{{ count1 }}" />
						</block>
