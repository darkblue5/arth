                                                    <view class="fenmu">
                                                        {{qstr.fm}}
                                                    </view>
                                                </view>
                                            </block>
                                        </view>
                                    </view>
                                    <view slot="right" class="cel-del-btn" data-quesid="{{item.id}}" data-qindex="{{index}}" bindtap="onDelQues">删除</view>
